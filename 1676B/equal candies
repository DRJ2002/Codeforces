#include<bits/stdc++.h>
using namespace std;
int main(){
int t;
cin>>t;
while(t--){
    int n;
    cin>>n;
    int drj[n];
    int sum=0;
    for(int i=0;i<n;i++){
        cin>>drj[i];
    }sort(drj,drj+n);
    for(int i=1;i<n;i++){
        if(drj[0]<drj[i]){
            sum+=drj[i]-drj[0];
        }
        
    }cout<<sum<<endl;
}return 0;
}
